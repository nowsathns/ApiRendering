
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.css">
<script src="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.js"></script>


@model ApiRendering.Models.RegistrationResponse
@{
    ViewBag.Title = "Vehicle Details Search";
}

<script>
    function hi() {
        swal({
            title: "Red Alert!",
            text: "I will close in 4 seconds.",
            timer: 4000,
            showConfirmButton: false
        });

    }

</script>

<h2><label style="color:darkblue"><b> Vehicle Search</b></label></h2>

@if (Model == null)
{
    <label> Could not find the information for the given number </label>
}
else
{
    <br />
    <label type="button" data-toggle="collapse" data-target="#demo2"><b><u>VEHICLE OWNER DETAILS</u></b></label>
    <br />
    <br />
    <div id="demo2" class="collapse">
        <label style="color:blue"><b> Owner Name:</b> </label> @Model.owner
        <br />
        <label style="color:blue"> <b>OwnerFathersName:</b> </label> @Model.ownerFathersName
        <br />
        <label style="color:blue"> <b>Present Address: </b> </label> @Model.permanentAddress.completeAddress
        <br />
        <label style="color:blue"> <b>addressLine:</b> </label> @Model.permanentAddress.addressLine
        <br />
        <br />
    </div>
    <label type="button" data-toggle="collapse" data-target="#demo1"><b><u>VEHICLE DETAILS</u></b></label>
    <br />
    <br />
    <div id="demo1" class="collapse">
        <label style="color:blue"> <b>Vehicle RC Status:</b> </label> @Model.registrationDetail.rcStatusAsOn
        <br />
        <label style="color:blue"> <b>Vehicle Authority State:</b> </label> @Model.registrationDetail.authority
        <br />
        <label style="color:blue"><b> expiryDate: </b></label> @Model.registrationDetail.expiryDate
        <br />
        <label style="color:blue"><b> Vehicle Number:</b> </label> @Model.vehicle.number
        <br />
        <label style="color:blue"> <b>companyName: </b></label> @Model.vehicle.companyName
        <br />
        <label style="color:blue"> <b>Model Name:</b> </label> @Model.vehicle.modelName
        <br />
        <label style="color:blue"><b> Vehilce Color:</b> </label> @Model.vehicle.color
        <br />
        <label style="color:blue"> <b>Vehilce engine:</b> </label> @Model.vehicle.engine
        <br />
        <br />
    </div>
    <label type="link" data-toggle="collapse" data-target="#demo"><b><u>VEHICLE INSURENCE DETAILS</u></b></label>
    <br />
    <br />
    <div id="demo" class="collapse">
        <label style="color:blue"> <b>Insurance No:</b> </label> @Model.insurance.policyNumber
        <br />
        <label style="color:blue"> <b>Insurance company:</b> </label> @Model.insurance.company
        <br />
        <label style="color:blue"> <b>Insurance ValidTill:</b> </label> @Model.insurance.validTill
        <br />
        <br />
    </div>
}