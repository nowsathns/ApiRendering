
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.css">
<script src="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.js"></script>


@model ApiRendering.Models.RegistrationResponse
@{
    ViewBag.Title = "Vehicle Details Search";
}

<script>
    function hi() {
        swal({
            title: "Red Alert!",
            text: "I will close in 4 seconds.",
            timer: 4000,
            showConfirmButton: false
        });

    }

</script>

<h2><label style="color:darkblue"><b> Vehicle Search</b></label></h2>

@if (Model == null)
{
    <label> Could not find the information for the given number </label>
}
else
{
    <br />
    <label type="button" data-toggle="collapse" data-target="#demo2"><b><u>VEHICLE OWNER DETAILS</u></b></label>
    <br />
    <br />
    <div id="demo2" class="collapse">
        <table class="table table-striped">
            <tr>
                <td scope="row"><b> Owner Name:</b></td>
                <td> @Model.owner</td>
            </tr>
            <tr>
                <td scope="row"><b>OwnerFathersName:</b></td>
                <td> @Model.ownerFathersName</td>
            </tr>
            <tr>
                <td scope="row"><b>Present Address: </b></td>
                <td> @(Model.permanentAddress != null ? Model.permanentAddress.completeAddress : "N/A" )</td>
            </tr>
            <tr>
                <td scope="row"><b>addressLine:</b></td>
                <td> @(Model.permanentAddress != null ? Model.permanentAddress.addressLine : "N/A" ) </td>
            </tr>

        </table>
    </div>
    <label type="button" data-toggle="collapse" data-target="#demo1"><b><u>VEHICLE DETAILS</u></b></label>
    <br />
    <br />
    <div id="demo1" class="collapse">
        <table class="table table-striped">
            <tr>
                <td scope="row"><b>Vehicle RC Status:</b></td>
                <td>@(Model.registrationDetail != null ? Model.registrationDetail.rcStatusAsOn : "N/A")</td>
            </tr>
            <tr>
                <td scope="row"> <b>Vehicle Authority State:</b></td>
                <td>@(Model.registrationDetail != null ? Model.registrationDetail.authority : "N/A" )</td>
            </tr>
            <tr>
                <td scope="row"><b> expiryDate: </b></td>
                <td>@(Model.registrationDetail != null ? Model.registrationDetail.expiryDate : "N/A")</td>
            </tr>
            <tr>
                <td scope="row"><b> Vehicle Number:</b></td>
                <td> @(Model.vehicle !=null ? Model.vehicle.number : "N/A")</td>
            </tr>
            <tr>
                <td scope="row"> <b>companyName: </b></td>
                <td> @(Model.vehicle!=null ? Model.vehicle.companyName : "N/A")</td>
            </tr>
            <tr>
                <td scope="row"><b>Model Name:</b></td>
                <td> @(Model.vehicle!=null ? Model.vehicle.modelName : "N/A") </td>
            </tr>
            <tr>
                <td scope="row"><b> Vehilce Color:</b></td>
                <td> @(Model.vehicle!=null ?Model.vehicle.color : "N/A")</td>
            </tr>
            <tr>
                <td scope="row"><b>Vehilce engine:</b></td>
                <td>@(Model.vehicle != null ? Model.vehicle.engine : "N/A")</td>
            </tr>
        </table>
    </div>
    <label type="link" data-toggle="collapse" data-target="#demo"><b><u>VEHICLE INSURENCE DETAILS</u></b></label>
    <br />
    <br />
    <div id="demo" class="collapse">
        <table class="table table-striped">
            <tr>
                <td scope="row"><b>Insurance No:</b></td>
                <td> @(Model.insurance !=null ? Model.insurance.policyNumber : "N/A")</td>
            </tr>
            <tr>
                <td scope="row"><b>Insurance company:</b></td>
                <td>@(Model.insurance !=null ? Model.insurance.company : "N/A")</td>
            </tr>
            <tr>
                <td scope="row"><b>Insurance ValidTill:</b></td>
                <td> @(Model.insurance !=null ? Model.insurance.validTill : "N/A")</td>
            </tr>
        </table>
    </div>
}