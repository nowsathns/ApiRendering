<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.css">
<script src="https://cdnjs.cloudflare.com/ajax/libs/sweetalert/1.1.3/sweetalert.min.js"></script>
@model ApiRendering.Models.APIInput
@{

    ViewData["Title"] = "Vehicle Search";
}

<div style="padding-top:10px;min-height:300px;">
    <form action="~/Home/VehicleSearch" method="get">
        <b> Vehicle No</b>
        <br />
        @Html.TextBoxFor(model => model.vehicleNumber)
        <br />
        <br />
        <button class=" btn btn-primary mt-2" onclick="hi()" type="submit">Search</button>
    </form>
</div>
<script>
    function hi() {
        swal({
            title: "Information Loading",
            /*icon: "success",*/
            /* timer: 6000,*/
            showConfirmButton: false
        });


    }
</script>